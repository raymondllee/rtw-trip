# Web app config (used by scripts/build-web-config.js)

# Google Cloud (UI display and service configuration)
GOOGLE_CLOUD_PROJECT_ID=YOUR_GOOGLE_CLOUD_PROJECT_ID
GOOGLE_CLOUD_LOCATION=us-central1

# Google OAuth (client-side sign-in)
GOOGLE_OAUTH_CLIENT_ID=YOUR_GOOGLE_OAUTH_CLIENT_ID
GOOGLE_OAUTH_CLIENT_SECRET=YOUR_GOOGLE_OAUTH_CLIENT_SECRET

# Google Maps (client-side maps SDK)
GOOGLE_MAPS_API_KEY=YOUR_MAPS_API_KEY_HERE

# Firebase (client-side config)
FIREBASE_API_KEY=YOUR_FIREBASE_API_KEY_HERE
FIREBASE_AUTH_DOMAIN=YOUR_FIREBASE_AUTH_DOMAIN
FIREBASE_PROJECT_ID=YOUR_FIREBASE_PROJECT_ID
FIREBASE_STORAGE_BUCKET=YOUR_FIREBASE_STORAGE_BUCKET
FIREBASE_MESSAGING_SENDER_ID=YOUR_FIREBASE_MESSAGING_SENDER_ID
FIREBASE_APP_ID=YOUR_FIREBASE_APP_ID
FIREBASE_MEASUREMENT_ID=YOUR_FIREBASE_MEASUREMENT_ID

## Usage
# 1) Copy this file to .env and fill in real values.
# 2) Run:  node scripts/build-web-config.js
#    This generates web/config.js and web/firebase-config.js (both are gitignored).
